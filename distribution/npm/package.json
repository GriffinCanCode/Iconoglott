{
  "name": "@iconoglott/renderer",
  "version": "0.1.0",
  "description": "High-performance visual DSL renderer with WASM acceleration and WebSocket backend support",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.js"
    },
    "./client": {
      "types": "./dist/client/ws.d.ts",
      "import": "./dist/client/ws.js"
    },
    "./wasm": {
      "types": "./dist/wasm/bridge.d.ts",
      "import": "./dist/wasm/bridge.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "build:wasm": "cd ../../source && make wasm",
    "build:all": "npm run build:wasm && npm run build",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "svg",
    "dsl",
    "graphics",
    "visualization",
    "react",
    "ai",
    "websocket",
    "real-time",
    "wasm",
    "webassembly",
    "rust"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@vitest/coverage-v8": "^1.0.0",
    "fast-check": "^3.23.2",
    "react": "^18.2.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
